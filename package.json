{
    "name": "lemons-plugin-template",
    "version": "0.0.2",
    "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
    "main": "main.js",
    "scripts": {
        "dev": "bun esbuild.dev.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && bun esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "test": "bun test",
        "format": "prettier --write .",
        "lint": "eslint --max-warnings=0 src/**",
        "lint:fix": "eslint --max-warnings=0 --fix src/**",
        "commit": "bun run automation/commit.ts"
    },
    "keywords": [],
    "author": "Moritz Jung",
    "license": "GPL-3.0",
    "devDependencies": {
        "@lemons_dev/parsinom": "^0.0.10",
        "@types/node": "^20.3.2",
        "@types/ungap__structured-clone": "^0.3.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "builtin-modules": "^3.3.0",
        "bun-types": "^1.0.7",
        "esbuild": "^0.17.0",
        "esbuild-plugin-copy-watch": "^2.0.0",
        "esbuild-svelte": "^0.7.1",
        "eslint": "^8.52.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-isaacscript": "^3.5.6",
        "eslint-plugin-only-warn": "^1.1.0",
        "obsidian": "latest",
        "prettier": "3.0.3",
        "string-argv": "^0.3.2",
        "ts-jest": "^29.1.1",
        "tslib": "2.4.0",
        "typescript": "^5.2.2"
    }
}